{
    "compilerOptions": {
        "outDir": "./.tsbuild/",
        "sourceMap": false,
        // "target": "es2019",
        "module": "commonjs",
        "removeComments": true,
        "noImplicitAny": true,
        "strictPropertyInitialization": true,
        "strictNullChecks": true,
        "types": [
            "node",
            "webdriverio/async",
            "@wdio/mocha-framework",
            "expect-webdriverio"
        ],
        "target": "ES5",
        "resolveJsonModule": true,
        "esModuleInterop": true
    }
}